{
  "name": "stripe-integration-package",
  "version": "1.0.1",
  "description": "A reusable Stripe integration package for Node.js/Express with checkout sessions, webhooks, and revenue tracking.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist",
    "test": "jest",
    "prepare": "npm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "stripe",
    "payments",
    "checkout",
    "webhooks",
    "express",
    "typescript",
    "nodejs",
    "npm-package"
  ],
  "author": "Ashish Kumar <kaumatchobey@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashish-kumarsn/stripe-integration-package.git"
  },
  "bugs": {
    "url": "https://github.com/Ashish-kumarsn/stripe-integration-package/issues"
  },
  "homepage": "https://github.com/Ashish-kumarsn/stripe-integration-package#readme",
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
